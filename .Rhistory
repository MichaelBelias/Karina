model <- "
// One-Sample Order Restricted Comparsion of Means
data {
int<lower=0> ndata;
vector[ndata] x;
}
parameters {
real sigmatmp;
real<upper=0> delta;
}
transformed parameters {
real<lower=0> sigma;
real mu;
sigma <- fabs(sigmatmp);
mu <- delta * sigma;
}
model {
// Delta and sigma Come From (Half) Cauchy Distributions
sigmatmp ~ cauchy(0, 1);
delta ~ cauchy(0, 1)T[,0];
// Data
x ~ normal(mu, sigma);
}"
